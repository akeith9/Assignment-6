@{
}

@model Books

<tr>
    <td>@Model.Title</td>
    <td>@Model.FirstName</td>
    <td>@Model.MiddleName</td>
    <td>@Model.LastName</td>
    <td>@Model.Publisher</td>
    <td>@Model.ISBN</td>
    <td>@Model.Classification</td>
    <td>@Model.Category</td>
    <td>$@Model.Price</td>
    <td>@Model.Pages</td>
</tr>